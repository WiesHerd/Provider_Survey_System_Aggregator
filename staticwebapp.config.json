{
  "routes": [
    {
      "route": "/sample-survey.csv",
      "serve": "/sample-survey.csv",
      "statusCode": 200
    },
    {
      "route": "/synthetic_*.csv",
      "serve": "/{*}.csv",
      "statusCode": 200
    },
    {
      "route": "/static/*",
      "serve": "/static/*",
      "statusCode": 200
    },
    {
      "route": "/api/*",
      "serve": "/api/*",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/api/*", "/*.csv", "/static/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
